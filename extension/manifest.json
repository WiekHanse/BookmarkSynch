{
  "browser_specific_settings": {
    "gecko": {
      "id": "henri.wiek@hansemerkur.de"
    }
  },
  "manifest_version": 3,
  "name": "BookmarkImport",
  "version": "1.2.6",
  "author": "NotMeHenri",
  "description": "Eine Extension die Bookmarks in Firefox synchronisieren soll.",
  "background": {
    "scripts": ["background_script.js"]
  },
  "permissions": [
    "bookmarks",
    "tabs",
    "activeTab"
  ],
  "host_permissions": [
    "https://gitpro.hanse-merkur.de/*"
  ],
  "options_ui": {
    "page": "options.html"
  },
  "icons": {
    "48": "bookmark.jpg"
  }
}
